{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "client-adopt": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/client-adopt",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/assets/css/animate.css",
                            "src/assets/css/animsition.min.css",
                            "src/assets/css/bootstrap.css",
                            "src/assets/css/bootstrap.min.css",
                            "src/assets/css/bootstrap.mina.css",
                            "src/assets/css/bootstrapa.css",
                            "src/assets/css/daterangepicker.css",
                            "src/assets/css/font-awesome.min.css",
                            "src/assets/css/font-awesome.mina.css",
                            "src/assets/css/hamburgers.min.css",
                            "src/assets/css/icon-font.min.css",
                            "src/assets/css/main.css",
                            "src/assets/css/nouislider.min.css",
                            "src/assets/css/select2.min.css",
                            "src/assets/css/style.css",
                            "src/assets/css/stylea.css",
                            "src/assets/css/util.css",
                            "src/assets/css/utila.css",
                            "src/assets/css/material-design-iconic-font.min.css"
                        ],
                        "scripts": [
                            "src/assets/script/animsition.min.js",
                            "src/assets/script/bootstrap.bundle.min.js",
                            "src/assets/script/bootstrap.js",
                            "src/assets/script/bootstrap.min.js",
                            "src/assets/script/bootstrap.mina.js",
                            "src/assets/script/contactform.js",
                            "src/assets/script/countdowntime.js",
                            "src/assets/script/counterup.min.js",
                            "src/assets/script/custom.js",
                            "src/assets/script/daterangepicker.js",
                            "src/assets/script/easing.js",
                            "src/assets/script/hoverIntent.js",
                            "src/assets/script/jquery.min.js",
                            "src/assets/script/jquery.mina.js",
                            "src/assets/script/jquery-3.2.1.min.js",
                            "src/assets/script/lockfixed.min.js",
                            "src/assets/script/moment.min.js",
                            "src/assets/script/nouislider.min.js",
                            "src/assets/script/popper.js",
                            "src/assets/script/select2.min.js",
                            "src/assets/script/stellar.min.js",
                            "src/assets/script/sticky.js",
                            "src/assets/script/superfish.min.js",
                            "src/assets/script/tether.min.js",
                            "src/assets/script/waypoints.min.js",
                            "src/assets/script/wNumb.js",
                            "src/assets/script/jquery.validate.min.js",
                            "src/assets/script/additional-methods.min.js"

                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "2mb",
                                "maximumError": "5mb"
                            }]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "client-adopt:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "client-adopt:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "client-adopt:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "src/assets/css/animate.css",
                            "src/assets/css/animsition.min.css",
                            "src/assets/css/bootstrap.css",
                            "src/assets/css/bootstrap.min.css",
                            "src/assets/css/bootstrap.mina.css",
                            "src/assets/css/bootstrapa.css",
                            "src/assets/css/daterangepicker.css",
                            "src/assets/css/font-awesome.min.css",
                            "src/assets/css/font-awesome.mina.css",
                            "src/assets/css/hamburgers.min.css",
                            "src/assets/css/icon-font.min.css",
                            "src/assets/css/main.css",
                            "src/assets/css/nouislider.min.css",
                            "src/assets/css/select2.min.css",
                            "src/assets/css/style.css",
                            "src/assets/css/stylea.css",
                            "src/assets/css/util.css",
                            "src/assets/css/utila.css",
                            "src/assets/css/material-design-iconic-font.min.css"
                        ],
                        "scripts": [

                            "src/assets/script/animsition.min.js",
                            "src/assets/script/bootstrap.bundle.min.js",
                            "src/assets/script/bootstrap.js",
                            "src/assets/script/bootstrap.min.js",
                            "src/assets/script/bootstrap.mina.js",
                            "src/assets/script/contactform.js",
                            "src/assets/script/countdowntime.js",
                            "src/assets/script/counterup.min.js",
                            "src/assets/script/custom.js",
                            "src/assets/script/daterangepicker.js",
                            "src/assets/script/easing.js",
                            "src/assets/script/hoverIntent.js",
                            "src/assets/script/jquery.min.js",
                            "src/assets/script/jquery.mina.js",
                            "src/assets/script/jquery-3.2.1.min.js",
                            "src/assets/script/lockfixed.min.js",
                            "src/assets/script/moment.min.js",
                            "src/assets/script/nouislider.min.js",
                            "src/assets/script/popper.js",
                            "src/assets/script/select2.min.js",
                            "src/assets/script/stellar.min.js",
                            "src/assets/script/sticky.js",
                            "src/assets/script/superfish.min.js",
                            "src/assets/script/tether.min.js",
                            "src/assets/script/waypoints.min.js",
                            "src/assets/script/wNumb.js",
                            "src/assets/script/jquery.validate.min.js",
                            "src/assets/script/additional-methods.min.js"
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "client-adopt-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "client-adopt:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "client-adopt:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "client-adopt"
}